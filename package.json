{
	"name": "server",
	"version": "1.0.0",
	"private": true,
	"main": "lib/src/index.js",
	"author": "Grupo TecnoSchisto",
	"license": "MIT",
	"engines": {
		"node": "16.13.0"
	},
	"scripts": {
		"lint": "eslint src  data --ext .ts --fix",
		"dev": "cross-env NODE_ENV=development ts-node-dev --inspect --respawn --transpile-only src/index.ts",
		"create": "sucrase ./src -d ./lib/src --transforms typescript,imports",
		"postbuild": "sucrase ./data -d ./lib/data --transforms typescript,imports",
		"start": "node lib/src/index.js",
		"build": "ts-node-dev --inspect --respawn --transpile-only src/index.ts",
		"prod": "cross-env NODE_ENV=production ts-node-dev --inspect --respawn --transpile-only src/index.ts",
		"genDoc": "node swagger.js",
		"prelint": "yarn prettier src/**/*.ts *.ts  data/**/*.ts --write",
		"build:db:test": "cross-env NODE_ENV=test ts-node ./data/build",
		"build:db:prod": "cross-env NODE_ENV=production ts-node ./data/build",
		"build:db:dev": "cross-env NODE_ENV=development ts-node --transpile-only ./data/build",
		"postbuild:db:dev": "sequelize-cli db:seed:all",
		"dev:database": "ts-node-dev --inspect --respawn --transpile-only ./data "
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"md5": "^2.3.0",
		"pg": "^8.7.1",
		"sequelize": "^6.7.0",
		"swagger-autogen": "^2.11.2",
		"swagger-ui-express": "^4.1.6",
		"ts-node-dev": "^1.1.8",
		"swagger-jsdoc": "^6.1.0"
	},
	"devDependencies": {
		"@types/body-parser": "^1.19.1",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/node": "^16.10.4",
		"@types/pg": "^8.6.1",
		"@types/sequelize": "^4.28.10",
		"@types/swagger-jsdoc": "^6.0.1",
		"@types/swagger-ui-express": "^4.1.3",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.1.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import-helpers": "^1.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-unused-imports": "^1.1.5",
		"prettier": "^2.4.1",
		"sequelize-cli": "^6.2.0",
		"sucrase": "^3.20.2",
		"typescript": "^4.4.4"
	}
}
